<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush
        x:Key="PrimaryColorBrush"
        Color="#1E90FF" />
    <SolidColorBrush
        x:Key="FontColorBrush"
        Color="#333333" />
    <SolidColorBrush
        x:Key="SecondaryColorBrush"
        Color="#87CEFA" />
    <SolidColorBrush
        x:Key="DefaultButtonBrush"
        Color="#F0F8FF" />
    <SolidColorBrush
        x:Key="ButtonOnMouse"
        Color="#ADD8E6" />
    <SolidColorBrush
        x:Key="Success"
        Color="#2E8B57" />
    <SolidColorBrush
        x:Key="Alert"
        Color="#DC143C" />
</ResourceDictionary>