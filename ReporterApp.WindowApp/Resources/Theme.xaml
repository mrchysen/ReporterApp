<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush
        x:Key="PrimaryColorBrush"
        Color="#FFCC33" />
    <SolidColorBrush
        x:Key="FontColorBrush"
        Color="#000000" />
    <SolidColorBrush
        x:Key="SecondaryColorBrush"
        Color="#336699" />
    <SolidColorBrush
        x:Key="DefaultButtonBrush"
        Color="#FFDB7F" />
    <SolidColorBrush
        x:Key="ButtonOnMouse"
        Color="#FFB732" />
    <SolidColorBrush
        x:Key="Success"
        Color="#008000" />
    <SolidColorBrush
        x:Key="Alert"
        Color="#FF0000" />
</ResourceDictionary>